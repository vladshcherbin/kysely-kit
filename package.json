    "kysely": "^0.27.4",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@shcherbin/eslint-config-node-typescript": "^0.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.15",
    "@types/pg": "^8.11.6",
    "eslint": "^8.57.0",
    "kysely": "^0.27.4",
    "np": "^10.0.7",
