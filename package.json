{
  "name": "kysely-kit",
  "description": "Useful utilities for working with Kysely",
  "version": "0.7.4",
  "author": "Vlad Shcherbin <vlad.shcherbin@gmail.com>",
  "repository": "vladshcherbin/kysely-kit",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch --env-file=.env src/kysely.ts",
    "lint": "eslint src",
    "prepublishOnly": "pnpm lint && pnpm typecheck && pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "camelcase": "^8.0.0",
    "cosmiconfig": "^9.0.0",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.7",
    "environment": "^1.1.0",
    "prettier": "^3.4.2",
    "read-package-up": "^11.0.0",
    "sade": "^1.8.1",
    "ts-morph": "^25.0.0",
    "tsx": "^4.19.2",
    "valibot": "1.0.0-beta.12"
  },
  "peerDependencies": {
    "kysely": "^0.27.4",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@shcherbin/eslint-config-node-typescript": "^0.3.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.10",
    "eslint": "^9.18.0",
    "kysely": "^0.27.5",
    "pg": "^8.13.1",
    "typescript": "^5.7.3"
  },
  "files": [
    "build",
    "types"
  ],
  "bin": {
    "kysely": "build/kysely.js"
  },
  "types": "types/kysely.d.ts",
  "license": "MIT"
}
